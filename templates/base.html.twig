<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="{{ asset('css/normalize.css') }}">
    <link rel="icon" href="{{ asset('photo/iconesortir.png') }}">
    <link rel="stylesheet" href="{{ asset('css/normalize.css') }}">
    {# templates/base.html.twig #}

    {# beware that the blocks in your template may be named different #}
    {% block head_css %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <link rel="stylesheet" href="{{ asset('css/app.css') }}">
        <link rel="stylesheet" href="{{ asset('css/normalize.css') }}">
    {% endblock %}
    {% block head_js %}
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    {% endblock %}

    <title>{% block title %}Eni Sortie {% endblock %}</title>
</head>

<body>
        <header class="container">
            <div>
                <a href="{{ path('sortie_accueil') }}"><img src="{{ asset('photo/sortir.png') }}" alt="Logo App" class="logoEni col-lg-5 col-xs-12 img-fluid"></a>
            </div>
            <div class="navBarre col-lg-7 col-xs-12 text-nowrap d-none d-lg-block d-xl-block">
                {% include 'inc/nav.html.twig'%}
            </div>
        </header>

        {%for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="flash alert-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}


            {% block body %}
                Bienvenu sur Sortie.com !
            {% endblock %}

        <footer class="card-footer ">
            {% include 'inc/footer.html.twig' %}
        </footer>
</body>
</html>