{% extends 'base.html.twig' %}


{% block title %} {{ parent() }} | Ajouter une ville {% endblock %}

{% block body %}

    <main class="container-fluid">
        <h1 class="titreFormulaire">Ajouter une ville</h1>

        <div>
            {{ form_start(villeForm) }}
            {{ form_widget(villeForm) }}
            <div>
                <button name="cree" class="btn btn-primary" value="cree">Ajouter une ville</button>
            </div>
            <a class="btn btn-primary" href="javascript:history.go(-1)">Retour</a>
            {{ form_end(villeForm) }}
        </div>
    </main>

{% endblock %}