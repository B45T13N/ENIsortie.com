{% extends 'base.html.twig' %}

{% block body %}

    <form method="post" action="{{ path('admin_app_fichier') }}" enctype="multipart/form-data">
        <input type="file" name="fileCsv" class="btn btn-lg btn-primary">
        <button class="btn btn-lg btn-primary" value="btnCsv" type="submit">Import CSV</button>
        <br>
        <a class="btn btn-primary" href="javascript:history.go(-1)">Retour</a>
    </form>
{% endblock %}

{% block title %}
    {{ parent() }} | Insertion par fichier
{% endblock %}

